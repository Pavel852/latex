{
    "name": "pavel852/latex",
    "description": "A LaTeX integration library for PHP.",
    "homepage": "https://github.com/Pavel852/latex",
    "keywords": ["php", "latex", "utf-8"],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Pavel852/latex"
        }
    ],
    "autoload": {
        "files": ["latex.php"]
    },
    "authors": [
        {
            "name": "Pavel Bartos",
            "email": "pavel.bartos.pb@gmail.com",
            "orcid": "https://orcid.org/0009-0001-3558-4312",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/Pavel852/latex",
        "issues": "https://github.com/Pavel852/latex/issues",
        "license": "https://github.com/Pavel852/latex/blob/main/LICENSE",
        "docs": "https://github.com/Pavel852/latex/blob/main/README.md"
    },
    "require": {
        "php": ">=7.0"        
    },
    "scripts": {
        "post-install-cmd": [
            "php -r \"chmod('./tmp', 0777);\""
        ]
